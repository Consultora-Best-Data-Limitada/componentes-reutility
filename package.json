{
  "name": "componentes-reutility",
  "version": "0.0.0",
  "private": true,
  "main": "dist/componentes-reutility.umd.cjs",
  "module": "dist/componentes-reutility.js",
  "browser": "dist/componentes-reutility.js",
  "type": "module",
  "types": "./dist/src/plugin",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "exports": {
    ".": {
      "types": "./dist/src/plugin/index.d.ts",
      "import": "./dist/componentes-reutility.js",
      "require": "./dist/componentes-reutility.umd.cjs"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^4.2.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.60.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vue-tsc": "^1.2.0"
  }
}
